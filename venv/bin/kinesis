#!/Users/llopacki/Documents/data/ws62/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'kinesis==0.0.1a13','console_scripts','kinesis'
__requires__ = 'kinesis==0.0.1a13'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('kinesis==0.0.1a13', 'console_scripts', 'kinesis')()
    )
